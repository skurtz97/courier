{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"strict": true,
		"allowJs": false,
		"checkJs": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["es2020", "DOM"],
		"moduleResolution": "node",
		"module": "es2020",
		"resolveJsonModule": true,
		"sourceMap": true,
		"target": "es2020",
		"exactOptionalPropertyTypes": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"noUncheckedIndexedAccess":true,
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$components": ["src/components"],
			"$components/*": ["src/components/*"],
			"$routes": ["src/routes"],
			"$routes/*": ["src/routes/*"],
			"$stores": ["src/stores"],
			"$stores/*": ["src/stores/*"],
			"$styles": ["src/styles"],
			"$styles/*": ["src/styles/*"],
			"$types": ["src/types"],
			"$types/*": ["src/types/*"],
		},
		
	},

	
}
